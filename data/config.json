{
  "project": {
    "name": "Stadtmuseum Viewer",
    "quality": 1
  },
  "delimiter": ";",
  "loader": {
    "info": "data/info.md",
    "items": "data/data.csv",
    "timeline": "data/timeline.csv",
    "layouts": [
      {
        "title": "Time"
      },
      {
        "title": "Similarity",
        "url": "data/tsne.csv",
        "scale": 1.4
      },
      {
        "title": "Color Grid",
        "url": "data/grid.csv",
        "scale": 1.8
      }
    ],
    "textures": {
      "medium": {
        "size": 400,
        "url": "data/sprites/spritesheet.json"
      },
      "detail": {
        "size": 1024,
        "url": "data/1024/"
      },
      "big": {
        "size": 4096,
        "url": "data/4096/"
      }
    }
  },
  "style": {
    "canvasBackground": "#ffffff",
    "timelineBackground": "#f8f4e9"
  },
  "projection": {
    "columns": 3
  },
  "detail": {
    "structure": [
      {
        "name": "Titel",
        "source": "_Titel",
        "type": "text"
      },
      {
        "name": "Künstler:in",
        "source": "_Künstler:in",
        "type": "text"
      },
      {
        "name": "Datierung",
        "source": "yearExact",
        "type": "text"
      },
      {
        "name": "Technik",
        "source": "_Techniken",
        "type": "text"
      },
      {
        "name": "Inventarnummer",
        "source": "_Inventarnummer",
        "type": "text"
      }
    ]
  }
}
